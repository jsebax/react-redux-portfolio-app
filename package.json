{
  "name": "react-redux-portfolio-app",
  "version": "1.0.0",
  "description": "A portfolio template built with Redux and React",
  "scripts": {
    "slate": "rm -rf node_modules && npm install",
    "clean": "rm -rf dist",
    "start": "NODE_ENV=development node src/server/index.js --progress --colors --profile",
    "start-prod": "NODE_ENV=production node src/server/index.js --progress --colors --profile",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress --colors --profile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsebax/react-redux-portfolio-app.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "history": "^4.4.1",
    "react": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-runtime": "^6.18.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "merge": "^1.2.0",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.7.4",
